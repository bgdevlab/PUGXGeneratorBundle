{
    "name": "pugx/generator-bundle",
    "type": "symfony-bundle",
    "description": "Extension of SensioGeneratorBundle",
    "keywords": ["symfony2 form field"],
    "license": "LGPL-3.0+",
    "authors": [
        {
            "name": "Massimiliano Arione",
            "email": "garakkio@gmail.com",
            "homepage": "http://massimilianoarione.it"
        },
        {
            "name": "Leonardo Proietti",
            "email": "leonardo.proietti@gmail.com"
        }
    ],
    "require": {
        "sensio/generator-bundle": "2.3.*",
        "twig/extensions": "dev-master@dev"
    },
    "suggest": {
        "knplabs/knp-paginator-bundle": "Needed for optional pagination",
        "lexik/form-filter-bundle": "Needed for optional filters"
    },
    "autoload": {
        "psr-0": { "PUGX\\GeneratorBundle": "" }
    },
    "target-dir": "PUGX/GeneratorBundle",
    "repositories": {
        "twig-extensions": {
            "type": "vcs",
            "url": "https://github.com/bgdevlab/Twig-extensions"
        }
    }
}
